            <section id="keyboard_function_keys" class="main-section">
              <header>Keyboard Function Keys</header>
                <article>
                  <table class="table-multimedia">
                    <caption><code>Fn+Shift+ <i>below key</i></code></caption>
                    <thead><thead>
                      <tbody>
                        <tr class="table-row-bottom-border">
                          <th>A</th>
                          <td>Prompt profile value</td>
                          <th>N</th>
                          <td>Backlit mode : Wave mode (Default)</td>
                        </tr>
                        <tr class="table-row-bottom-border">
                          <th>B</th>
                          <td>Profile 1</td>
                          <th>O</th>
                          <td>Backlit mode : Color Cycle mode</td>
                        </tr>
                        <tr class="table-row-bottom-border">
                          <th>C</th>
                          <td>Profile 2</td>
